<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiaoyuan.mapper.ErpPriceMapper" >
	<select id="selectByPriceMsg" resultType="com.xiaoyuan.model.ErpPrice">
		select goods_price from tl_erp_price
		where
		supplier_id=#{supplier_id}
		and
		goods_name =#{goods_name} 
	</select>
</mapper>